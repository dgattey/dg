{
  "name": "dg",
  "version": "0.0.0-development",
  "description": "Dylan Gattey's public website, implemented in Gatsby",
  "main": "index.js",
  "repository": "https://github.com/dgattey/dg.git",
  "homepage": "https://github.com/dgattey/dg#readme",
  "author": "Dylan Gattey <hi@dylangattey.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "gatsby": "^2.32.3",
    "gatsby-image": "^2.11.0",
    "gatsby-plugin-manifest": "^2.12.0",
    "gatsby-plugin-offline": "^3.10.0",
    "gatsby-plugin-react-helmet": "^3.10.0",
    "gatsby-plugin-sharp": "^2.14.1",
    "gatsby-source-filesystem": "^2.11.0",
    "gatsby-transformer-sharp": "^2.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "regenerator-runtime": "^0.13.7"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "gatsby-cli": "^2.19.1",
    "prettier": "2.2.1",
    "semantic-release": "^17.3.9"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/github",
      "@semantic-release/git",
      "@semantic-release/changelog"
    ]
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop -H 0.0.0.0",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,yml}\" \"*.{js,jsx,ts,tsx,json,md,yml}\" \"!node_modules/**/*\" \"!public/**/*\" \"!.cache/**/*\" \"!jekyll-archive/**/*\"",
    "start": "yarn run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "semantic-release": "semantic-release"
  }
}
