{
  "name": "dg",
  "version": "0.0.0-development",
  "description": "Dylan Gattey's public website, implemented in NextJS",
  "repository": "https://github.com/dgattey/dg.git",
  "homepage": "https://github.com/dgattey/dg#readme",
  "author": "Dylan Gattey <hi@dylangattey.com>",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "dev": "SENTRY_IGNORE_API_RESOLUTION_ERROR=1 next dev",
    "build": "NEXT_PUBLIC_APP_VERSION=$(pnpm -s version:fetch) next build",
    "build:analyze": "ANALYZE=true pnpm build",
    "serve": "next start",
    "release": "semantic-release",
    "format": "prettier --loglevel warn --write \"**/*.{js,ts,tsx,css,md,json,yml}\"",
    "lint": "eslint . --cache --fix --ext .js,.ts,.tsx --no-eslintrc -c config/eslint.config.js",
    "lint:types": "tsc --noEmit",
    "codegen": "DOTENV_CONFIG_PATH='.env.development.local' graphql-codegen --require dotenv/config --config codegen.js",
    "db": "sequelize",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:status": "sequelize db:migrate:status",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "db:generate": "sequelize migration:generate",
    "db:connect": "pscale connect dg --port 3309",
    "webhooks": "NODE_ENV=development node --require tsm scripts/webhooks.ts",
    "webhooks:local": "cloudflared tunnel run dg",
    "webhooks:create": "pnpm -s webhooks create",
    "webhooks:list": "pnpm -s webhooks list",
    "webhooks:delete": "pnpm -s webhooks delete",
    "version:fetch": "node --require tsm scripts/getVersion.ts",
    "clean": "rm -r .next"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "15.16.2",
    "@contentful/rich-text-types": "16.0.2",
    "@dg": "link:./src",
    "@fortawesome/fontawesome-svg-core": "6.2.1",
    "@fortawesome/free-brands-svg-icons": "6.2.1",
    "@fortawesome/free-solid-svg-icons": "6.2.1",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@next/bundle-analyzer": "13.0.6",
    "@picocss/pico": "1.5.6",
    "@sentry/integrations": "7.23.0",
    "@sentry/nextjs": "7.23.0",
    "animate-css-grid": "1.4.3",
    "cmd-ts": "0.11.0",
    "dotenv": "16.0.3",
    "eslint-config-dg": "link:./eslint-plugin-dg",
    "eslint-plugin-dg": "link:./eslint-plugin-dg",
    "graphql": "16.6.0",
    "graphql-request": "5.0.0",
    "lucide-react": "0.103.0",
    "mapbox-gl": "2.11.0",
    "mysql2": "2.3.3",
    "next": "13.0.6",
    "node-fetch": "3.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intersection-observer": "9.4.1",
    "react-is": "18.2.0",
    "react-map-gl": "7.0.20",
    "reflect-metadata": "0.1.13",
    "sequelize": "6.26.0",
    "sequelize-typescript": "2.1.5",
    "sharp": "0.31.2",
    "styled-components": "5.3.6",
    "swr": "1.3.0",
    "webpack": "5.75.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.15.0",
    "@graphql-codegen/near-operation-file-preset": "2.4.4",
    "@graphql-codegen/typescript": "2.8.3",
    "@graphql-codegen/typescript-operations": "2.5.8",
    "@semantic-release/changelog": "6.0.2",
    "@sentry/cli": "2.10.0",
    "@sentry/types": "7.23.0",
    "@types/mapbox-gl": "2.7.10",
    "@types/node": "18.11.10",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@types/styled-components": "5.1.26",
    "@types/validator": "13.7.10",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "eslint": "8.29.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-next": "13.0.6",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.2",
    "lint-staged": "13.0.4",
    "micromatch": "4.0.5",
    "postcss": "8.4.19",
    "postcss-syntax": "0.36.2",
    "prettier": "2.8.0",
    "semantic-release": "19.0.5",
    "sequelize-cli": "6.5.2",
    "tsm": "2.3.0",
    "typescript": "4.9.3"
  },
  "engines": {
    "node": ">=16"
  },
  "packageManager": "pnpm@7.16.1"
}
