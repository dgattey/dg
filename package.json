{
  "name": "dg",
  "version": "0.0.0-development",
  "description": "Dylan Gattey's public website, implemented in NextJS",
  "repository": "https://github.com/dgattey/dg.git",
  "homepage": "https://github.com/dgattey/dg#readme",
  "author": "Dylan Gattey <hi@dylangattey.com>",
  "license": "MIT",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "lefthook install",
    "dev": "VERCEL_GIT_COMMIT_SHA=$(git rev-parse HEAD) next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "serve": "next start",
    "release": "semantic-release",
    "format": "prettier --log-level warn --write \"**/*.{js,ts,tsx,mjs,cjs,css,md,json,yml,yaml}\"",
    "lint": "eslint . --cache --fix --ext .js,.ts,.tsx,.mjs,.cjs --no-eslintrc -c config/eslint.config.js",
    "lint:types": "tsc",
    "codegen": "DOTENV_CONFIG_PATH='.env.development.local' graphql-codegen --require dotenv/config --config codegen.ts",
    "db": "sequelize",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:status": "sequelize db:migrate:status",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "db:generate": "sequelize migration:generate",
    "db:connect": "pscale connect dg --port 3309",
    "webhooks": "NODE_ENV=development node --require tsm scripts/webhooks.ts",
    "webhooks:local": "cloudflared tunnel run dg",
    "webhooks:create": "pnpm -s webhooks create",
    "webhooks:list": "pnpm -s webhooks list",
    "webhooks:delete": "pnpm -s webhooks delete",
    "clean": "rm -r .next"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "15.17.2",
    "@contentful/rich-text-types": "16.3.0",
    "@emotion/cache": "11.11.0",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@fortawesome/fontawesome-svg-core": "6.4.2",
    "@fortawesome/free-brands-svg-icons": "6.4.2",
    "@fortawesome/free-solid-svg-icons": "6.4.2",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@mui/material": "5.14.9",
    "@next/bundle-analyzer": "13.4.19",
    "animate-css-grid": "1.5.1",
    "cmd-ts": "0.13.0",
    "dotenv": "16.3.1",
    "eslint-config-dg": "link:./eslint-plugin-dg",
    "eslint-plugin-dg": "link:./eslint-plugin-dg",
    "graphql": "16.8.0",
    "graphql-request": "6.1.0",
    "lefthook": "1.4.11",
    "lucide-react": "0.277.0",
    "mapbox-gl": "2.15.0",
    "mysql2": "3.6.1",
    "next": "13.4.19",
    "node-fetch": "3.3.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intersection-observer": "9.5.2",
    "react-is": "18.2.0",
    "react-map-gl": "7.1.6",
    "reflect-metadata": "0.1.13",
    "sequelize": "6.33.0",
    "sequelize-typescript": "2.1.5",
    "sharp": "0.32.5",
    "swr": "2.2.2",
    "use-breakpoint": "3.1.1",
    "webpack": "5.88.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/near-operation-file-preset": "2.5.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-operations": "4.0.1",
    "@semantic-release/changelog": "6.0.3",
    "@types/mapbox-gl": "2.7.14",
    "@types/micromatch": "4.0.2",
    "@types/node": "18.17.1",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@types/validator": "13.11.1",
    "@typescript-eslint/eslint-plugin": "6.7.0",
    "@typescript-eslint/parser": "6.7.0",
    "eslint": "8.49.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "postcss": "8.4.29",
    "postcss-syntax": "0.36.2",
    "prettier": "3.0.3",
    "semantic-release": "21.1.1",
    "sequelize-cli": "6.6.1",
    "tsm": "2.3.0",
    "turbo": "1.10.14",
    "typescript": "5.2.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@8.7.5"
}
