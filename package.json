{
  "name": "dg",
  "version": "0.0.0-development",
  "description": "Dylan Gattey's public website, implemented in Gatsby",
  "repository": "https://github.com/dgattey/dg.git",
  "homepage": "https://github.com/dgattey/dg#readme",
  "author": "Dylan Gattey <hi@dylangattey.com>",
  "license": "MIT",
  "private": true,
  "keywords": [
    "gatsby"
  ],
  "dependencies": {
    "gatsby": "^3.3.1",
    "gatsby-plugin-image": "^1.3.1",
    "gatsby-plugin-manifest": "^3.3.0",
    "gatsby-plugin-offline": "^4.3.0",
    "gatsby-plugin-react-helmet": "^4.3.0",
    "gatsby-plugin-sass": "^4.3.0",
    "gatsby-plugin-sharp": "^3.3.1",
    "gatsby-plugin-sitemap": "^3.3.0",
    "gatsby-plugin-typegen": "^2.2.4",
    "gatsby-source-contentful": "^5.3.0",
    "gatsby-source-filesystem": "^3.3.0",
    "gatsby-source-local-git": "^1.2.2",
    "gatsby-transformer-sharp": "^3.3.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "sass": "^1.32.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/react-helmet": "^6.1.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-tsc": "^2.0.0",
    "gatsby-plugin-ts-config": "^1.1.5",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "semantic-release": "^17.4.2",
    "typescript": "^4.2.4"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "type": "chore",
              "release": "patch"
            },
            {
              "type": "build",
              "release": "patch"
            },
            {
              "type": "test",
              "release": "patch"
            },
            {
              "type": "ci",
              "release": "patch"
            },
            {
              "type": "fix",
              "release": "patch"
            },
            {
              "type": "feat",
              "release": "minor"
            }
          ],
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES"
            ]
          }
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  },
  "scripts": {
    "develop": "gatsby develop -H 0.0.0.0",
    "start": "gatsby develop -H 0.0.0.0",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "semantic-release": "semantic-release",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --cache --fix --ext .js,.ts,.tsx",
    "format": "prettier --loglevel warn --write \"**/*.{js,ts,tsx,scss,css,md,json,yml}\" \"!node_modules/**/*\" \"!public/**/*\" \"!.cache/**/*\" \"!jekyll-archive/**/*\"",
    "prepare": "husky install",
    "posttest": "yarn format",
    "pretest": "yarn lint && yarn type-check"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --cache --fix --plugin tsc --rule 'tsc/config: [2, {configFile: \"./tsconfig.json\"}]'",
      "prettier --write"
    ],
    "*.{js}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{json,md,yml}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=14"
  }
}
