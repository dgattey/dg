{
  "name": "dg",
  "version": "0.0.0-development",
  "description": "Dylan Gattey's public website, implemented in NextJS",
  "repository": "https://github.com/dgattey/dg.git",
  "homepage": "https://github.com/dgattey/dg#readme",
  "author": "Dylan Gattey <hi@dylangattey.com>",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "build": "yarn db:generate && NEXT_PUBLIC_APP_VERSION=$(yarn version) next build",
    "start": "next start",
    "release": "semantic-release",
    "format": "prettier --loglevel warn --write \"**/*.{js,ts,tsx,css,md,json,yml}\"",
    "lint": "eslint . --cache --fix --ext .js,.ts,.tsx",
    "lint:styles": "stylelint 'src/**/*.{ts,tsx,js,jsx}' --fix",
    "lint:types": "tsc --noEmit",
    "codegen": "DOTENV_CONFIG_PATH='.env.development.local' graphql-codegen --require dotenv/config --config codegen.js",
    "db:local": "pscale connect dg --port 3309",
    "db:sync": "yarn pnpify prisma db push",
    "db:generate": "yarn pnpify prisma generate",
    "db:ui": "yarn pnpify prisma studio",
    "webhooks": "NODE_ENV=development esrun scripts/webhooks.ts",
    "webhooks:local": "cloudflared tunnel run dg",
    "webhooks:create": "yarn webhooks create",
    "webhooks:list": "yarn webhooks list",
    "webhooks:delete": "yarn webhooks delete",
    "version": "yarn esrun scripts/getVersion.ts",
    "clean": "rm -r .next"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^15.11.1",
    "@contentful/rich-text-types": "^15.11.1",
    "@picocss/pico": "^1.4.4",
    "@prisma/client": "3.8.1",
    "@sentry/integrations": "^6.17.3",
    "@sentry/nextjs": "^6.17.3",
    "animate-css-grid": "^1.4.3",
    "api": "link:./src/api::locator=dg%40workspace%3A.",
    "components": "link:./src/components::locator=dg%40workspace%3A.",
    "graphql": "^15.8.0",
    "graphql-request": "^3.7.0",
    "hooks": "link:./src/hooks::locator=dg%40workspace%3A.",
    "mapbox-gl": "^2.6.1",
    "next": "^12.0.9",
    "pages": "link:./src/pages::locator=dg%40workspace%3A.",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-intersection-observer": "^8.33.1",
    "react-is": "^17.0.2",
    "react-map-gl": "^7.0.0-beta.1",
    "react-resize-aware": "^3.1.1",
    "styled-components": "^5.3.3",
    "swr": "1.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@digitak/esrun": "^3.1.2",
    "@graphql-codegen/cli": "^2.4.0",
    "@graphql-codegen/near-operation-file-preset": "^2.2.3",
    "@graphql-codegen/typescript": "^2.4.2",
    "@graphql-codegen/typescript-operations": "^2.2.2",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@sentry/cli": "^1.72.1",
    "@stylelint/postcss-css-in-js": "^0.37.2",
    "@types/mapbox-gl": "^2.6.0",
    "@types/node": "^17.0.13",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.21",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "@yarnpkg/pnpify": "^3.1.1-rc.13",
    "cmd-ts": "^0.9.0",
    "dotenv": "^14.3.2",
    "eslint": "^8.8.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-next": "^12.0.9",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-tsc": "^2.0.0",
    "graphql-tag": "^2.12.6",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.2",
    "node-fetch": "^3.2.0",
    "postcss": "^8.4.6",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.5.1",
    "prisma": "3.8.1",
    "semantic-release": "^19.0.2",
    "stylelint": "^14.3.0",
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^4.5.5"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "type": "chore",
              "release": "patch"
            },
            {
              "type": "build",
              "release": "patch"
            },
            {
              "type": "test",
              "release": "patch"
            },
            {
              "type": "ci",
              "release": "patch"
            },
            {
              "type": "fix",
              "release": "patch"
            },
            {
              "type": "feat",
              "release": "minor"
            }
          ],
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES"
            ]
          }
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  },
  "engines": {
    "node": ">=14"
  },
  "packageManager": "yarn@3.1.1",
  "dependenciesMeta": {
    "react-dom@17.0.2": {
      "unplugged": true
    }
  },
  "resolutions": {
    "ansi-regex": "^5.0.1",
    "postcss": "^8.2.13"
  }
}
