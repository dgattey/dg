{
  "dependencies": {
    "@dg/content-models": "workspace:*",
    "@dg/shared-core": "workspace:*",
    "@types/pg": "8.16.0",
    "dotenv-mono": "catalog:",
    "pg": "8.17.2",
    "sequelize": "6.37.7",
    "sequelize-typescript": "2.1.6"
  },
  "devDependencies": {
    "@dg/testing": "workspace:*",
    "@dg/tsconfig": "workspace:*",
    "@types/jest": "catalog:",
    "jest": "catalog:",
    "pg-hstore": "2.3.4",
    "sequelize-cli": "6.6.5",
    "tsx": "catalog:",
    "typescript": "catalog:"
  },
  "main": "client.ts",
  "name": "@dg/db",
  "private": true,
  "scripts": {
    "check": "biome check --write .",
    "check:ci": "biome ci .",
    "check:types": "tsc",
    "clean": "rm -rf .turbo *.tsbuildinfo",
    "db": "NODE_OPTIONS='--import tsx' pnpm exec sequelize-cli",
    "migrate": "tsx scripts/runMigrate.ts",
    "migrate:generate": "NODE_OPTIONS='--import tsx' pnpm exec sequelize-cli migration:generate",
    "migrate:status": "NODE_OPTIONS='--import tsx' pnpm exec sequelize-cli db:migrate:status",
    "migrate:undo": "NODE_OPTIONS='--import tsx' pnpm exec sequelize-cli db:migrate:undo"
  },
  "types": "client.ts"
}
