{
  "dependencies": {
    "@dg/content-models": "workspace:*",
    "@dg/shared-core": "workspace:*",
    "@types/pg": "8.16.0",
    "dotenv-mono": "catalog:",
    "pg": "8.17.2",
    "sequelize": "6.37.7",
    "sequelize-typescript": "2.1.6"
  },
  "devDependencies": {
    "@dg/tsconfig": "workspace:*",
    "pg-hstore": "2.3.4",
    "sequelize-cli": "6.6.5",
    "tsx": "catalog:",
    "typescript": "catalog:"
  },
  "main": "client.ts",
  "name": "@dg/db",
  "private": true,
  "scripts": {
    "check": "biome check --write .",
    "check:ci": "biome ci .",
    "check:types": "tsc",
    "clean": "rm -rf .turbo *.tsbuildinfo",
    "db": "NODE_OPTIONS='--import tsx' pnpm exec sequelize-cli",
    "migrate": "NODE_OPTIONS='--import tsx' pnpm exec sequelize-cli db:migrate"
  },
  "types": "client.ts"
}
