    "@fortawesome/react-fontawesome": "0.2.0",
    "@mui/material": "5.14.9",
    "@next/bundle-analyzer": "13.4.19",
    "animate-css-grid": "1.5.1",
    "dotenv-mono": "github:dgattey/dotenv-mono#dgattey--fix-priority",
